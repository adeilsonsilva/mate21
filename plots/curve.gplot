#!/usr/bin/gnuplot

set term svg enhanced background rgb 'white'
set output "result.svg"
set grid
set xtics 100
set ytics 5
set xlabel "Epochs"
set ylabel "Values"
set title "MLP"
set key right bottom
set border lw 0.25
plot \
    "mlp.csv" using 1:2 dashtype 2 with lines title "Loss",\
    "mlp.csv" using 1:3 with lines title "ACC",\
